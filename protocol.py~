#coding:utf-8
SCAN_SINGLE = 0x01010100  

HEART_BEAT = 0x03020300
ATTRIBUTE = 0x03020100
ERROR = 0X03020101

RESET = 0x03010100
REMOVE = 0x03010200
MODIFYAP = 0x03010300
MODIFYIP = 0x03010400
SLEEP = 0x03010500
NOTIFY_NETCOMMUNITY = 0x03010600
NOTIFY_IP = 0x03010700
SWITCH_MODE = 0x03010900

STOP_CMD = 0x07000000

CMD = 0
DATA = 1
ACK = 2
